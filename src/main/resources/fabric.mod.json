{
  "schemaVersion": 1,
  "id": "paper-comet",
  "version": "${version}",
  "name": "Paper Comet",
  "description": "Meteor Client addon that implements useful features",
  "authors": [
    "Origaming_"
  ],
  "contact": {
    "repo": "https://github.com/OrigamingWasTaken/paper-comet"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "ch.origaming.papercomet.PaperComet"
    ]
  },
  "mixins": [
    "papercomet.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "241, 241, 241"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
